<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>....</th>
      <th>Status</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user %></td>
        <td></td>
        <td>
          <% if user.loggedin == 1 %>
            Online 
          <% else %>
            Offline   
          <% end %></td>
        <td></td>
     </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
