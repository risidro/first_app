<h1>Listing microposts</h1>

<table>
  <thead>
    <tr>
      <th>Story</th>
      <th>Sent1</th>
      <th>Sent2</th>
      <th>Sent3</th>
      <th>Sent4</th>
      <th>Sent5</th>
      <th>Sent6</th>
      <th>Sent7</th>
      <th>Sent8</th>
      <th>Sent9</th>
      <th>Sent10</th>
      <th>Sent11</th>
      <th>Sent12</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @microposts.each do |micropost| %>
      <tr>
        <td><%= micropost.STORY %></td>
        <td><%= micropost.sent1 %></td>
        <td><%= micropost.sent2 %></td>
        <td><%= micropost.sent3 %></td>
        <td><%= micropost.sent4 %></td>
        <td><%= micropost.sent5 %></td>
        <td><%= micropost.sent6 %></td>
        <td><%= micropost.sent7 %></td>
        <td><%= micropost.sent8 %></td>
        <td><%= micropost.sent9 %></td>
        <td><%= micropost.sent10 %></td>
        <td><%= micropost.sent11 %></td>
        <td><%= micropost.sent12 %></td>
        <td><%= link_to 'Show', micropost %></td>
        <td><%= link_to 'Edit', edit_micropost_path(micropost) %></td>
        <td><%= link_to 'Destroy', micropost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Micropost', new_micropost_path %>
